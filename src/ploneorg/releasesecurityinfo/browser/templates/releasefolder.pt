      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="ploneorg.releasesecurityinfo"
      metal:use-macro="context/main_template/macros/master">

  

  <metal:block fill-slot="main">

    <!--
    Refused to display 'https://launchpad.net/plone/+series' in a frame because it set 'X-Frame-Options' to 'SAMEORIGIN' - ERROR
    -->

    <iframe src="https://launchpad.net/plone/+series"
        width="100%" height="500" frameborder="1"
        allowfullscreen sandbox>
        <p>
        <a href="https://launchpad.net/plone/+series">
            Fallback link for browsers that don't support iframes
        </a>
        </p>
    </iframe>



  </metal:block>

</html>

